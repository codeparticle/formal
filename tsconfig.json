{
  "compilerOptions": {
    "moduleResolution": "node",
    "module": "esnext",
    "target": "es5",
    "lib": [
      "dom",
      "esnext"
    ],
    "strict": false,
    "noImplicitAny": false,
    "downlevelIteration": true,
    "allowSyntheticDefaultImports": true,
    "strictNullChecks": false,
    "esModuleInterop": true,
    "jsx": "react",
    "sourceMap": true,
    "stripInternal": true,
    "declaration": true,
    "declarationMap": true,
    "declarationDir": "dist/types",
    "outDir": "dist/esm5",
    "resolveJsonModule": true
  },
  "include": [
    "src"
  ],
  "compileOnSave": false
}
